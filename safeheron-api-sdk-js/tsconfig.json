{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
    },
    "types": [
      "jest",
      "node"
    ],
    "declaration": true,
    "outDir": "./lib",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src/"],
  "exclude": ["node_modules", "./src/_test_/**/*"]
}
